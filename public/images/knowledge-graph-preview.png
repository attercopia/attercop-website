  <!-- Nodes and Connections -->
  <!-- Projects (Deep Teal) -->
  <circle cx="320" cy="240" r="30" fill="#1B4D5C" opacity="0.9"/>
  <circle cx="180" cy="180" r="25" fill="#1B4D5C" opacity="0.8"/>
  <circle cx="460" cy="180" r="25" fill="#1B4D5C" opacity="0.8"/>

  <!-- Lessons (Sea Green) -->
  <circle cx="240" cy="340" r="25" fill="#6DBAA1" opacity="0.9"/>
  <circle cx="400" cy="340" r="25" fill="#6DBAA1" opacity="0.9"/>
  <circle cx="320" cy="380" r="20" fill="#6DBAA1" opacity="0.8"/>

  <!-- Issues (Coral) -->
  <circle cx="120" cy="280" r="20" fill="#E97B7A" opacity="0.8"/>
  <circle cx="520" cy="280" r="20" fill="#E97B7A" opacity="0.8"/>

  <!-- Teams (Ocean) -->
  <circle cx="180" cy="120" r="18" fill="#4A90A4" opacity="0.8"/>
  <circle cx="460" cy="120" r="18" fill="#4A90A4" opacity="0.8"/>

