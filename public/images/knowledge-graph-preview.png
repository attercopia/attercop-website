  <line x1="320" y1="240" x2="400" y2="340" stroke="#D1D5DB" stroke-width="2" opacity="0.6"/>
  <line x1="320" y1="240" x2="180" y2="180" stroke="#D1D5DB" stroke-width="2" opacity="0.6"/>
  <line x1="320" y1="240" x2="460" y2="180" stroke="#D1D5DB" stroke-width="2" opacity="0.6"/>
  <line x1="320" y1="240" x2="120" y2="280" stroke="#D1D5DB" stroke-width="2" opacity="0.6"/>
  <line x1="320" y1="240" x2="520" y2="280" stroke="#D1D5DB" stroke-width="2" opacity="0.6"/>
  <line x1="180" y1="180" x2="180" y2="120" stroke="#D1D5DB" stroke-width="1.5" opacity="0.5"/>
  <line x1="460" y1="180" x2="460" y2="120" stroke="#D1D5DB" stroke-width="1.5" opacity="0.5"/>
  <line x1="180" y1="180" x2="100" y2="200" stroke="#D1D5DB" stroke-width="1.5" opacity="0.5"/>
  <line x1="460" y1="180" x2="540" y2="200" stroke="#D1D5DB" stroke-width="1.5" opacity="0.5"/>
  <line x1="180" y1="180" x2="260" y2="140" stroke="#D1D5DB" stroke-width="1.5" opacity="0.5"/>
  <line x1="460" y1="180" x2="380" y2="140" stroke="#D1D5DB" stroke-width="1.5" opacity="0.5"/>

